CREATE TABLE grp (
    number integer NOT NULL
);
SEQUENCE grp_number_seq OWNED BY grp.number;
CREATE TABLE hah (
    xd character(1) NOT NULL
);
CREATE TABLE mah (
    xd character(1)
);
CREATE TABLE student (
    id integer NOT NULL,
    name character varying(64) DEFAULT ''::character varying NOT NULL,
    specification character(3) NOT NULL,
    year integer NOT NULL,
    login character(8) DEFAULT ''::bpchar NOT NULL,
    grp integer NOT NULL
);
SEQUENCE student_id_seq OWNED BY student.id;
CREATE TABLE tester (
    a integer,
    b integer,
    c integer,
    d integer
);
CREATE TABLE wah (
    xd character varying
);
ALTER TABLE ONLY grp
    ADD CONSTRAINT grp_number_key UNIQUE (number);
ALTER TABLE ONLY hah
    ADD CONSTRAINT hah_pkey PRIMARY KEY (xd);
ALTER TABLE ONLY student
    ADD CONSTRAINT student_pkey PRIMARY KEY (id);
ALTER TABLE ONLY tester
    ADD CONSTRAINT tester_d_key UNIQUE (d);
ALTER TABLE ONLY tester
    ADD CONSTRAINT tester_d_key2 UNIQUE (a, b, c);
ALTER TABLE ONLY student
    ADD CONSTRAINT student_grp_fkey FOREIGN KEY (grp) REFERENCES grp(number);
