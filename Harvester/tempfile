CREATE TABLE t1 (
    p1 integer NOT NULL,
    p2 integer NOT NULL,
    p3 character varying(5)
);
SEQUENCE t1_p1_seq OWNED BY t1.p1;
CREATE TABLE t2 (
    p1 bit(8),
    p2 integer
);
CREATE TABLE t3 (
    p1 bigint NOT NULL
);
SEQUENCE t3_p1_seq OWNED BY t3.p1;
ALTER TABLE ONLY t1
    ADD CONSTRAINT t1_pkey PRIMARY KEY (p2);
ALTER TABLE ONLY t2
    ADD CONSTRAINT t2_p2_fkey FOREIGN KEY (p2) REFERENCES t1(p2);
